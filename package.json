    "serve": "node dist/server.js",
    "dev": "nodemon --exec ts-node server.ts"
  },
  "keywords": ["ollama", "proxy", "cors"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "node-fetch": "^2.6.9"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17", 
    "@types/node": "^18.15.11",
    "@types/node-fetch": "^2.6.3",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
